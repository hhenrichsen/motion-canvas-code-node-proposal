{
  "name": "motion-canvas-component-library",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "prepare": "husky install",
    "build:dev": "rollup -c rollup.config.mjs",
    "watch": "rollup -c rollup.config.mjs -w",
    "build": "rollup -c rollup.config.mjs",
    "prebuild": "rimraf ./lib",
    "lint-staged": "lint-staged",
    "lint": "npm run eslint && npm run prettier",
    "format": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint \"**/*.ts?(x)\"",
    "eslint:fix": "eslint --fix \"**/*.ts?(x)\"",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "serve": "vite",
    "test": "vitest"
  },
  "unpkg": "lib/bundle.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "motion-canvas"
  ],
  "author": "",
  "homepage": "",
  "dependencies": {
    "@codemirror/language": "^6.10.1",
    "@lezer/common": "^1.2.1",
    "@lezer/highlight": "^1.2.0",
    "@lezer/javascript": "^1.4.13",
    "@lezer/lr": "^1.4.0",
    "@motion-canvas/2d": "^3.14.2",
    "@motion-canvas/core": "^3.14.1",
    "code-fns": "^0.11.0"
  },
  "devDependencies": {
    "@motion-canvas/ui": "^3.14.2",
    "@motion-canvas/vite-plugin": "^3.14.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "puppeteer": "^22.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.11.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-node-externals": "^7.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,scss,json,mjs}": "prettier --write"
  }
}
